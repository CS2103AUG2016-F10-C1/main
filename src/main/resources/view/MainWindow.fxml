<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tars.ui.MainWindow">
   <children>
      
      <AnchorPane fx:id="infoHeaderPlaceholder" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="692.0" styleClass="background" VBox.vgrow="NEVER" />
      <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.5" VBox.vgrow="ALWAYS">
         <items>
            <VBox fx:id="taskList" prefHeight="0.0">
                <padding>
                    <Insets bottom="10.0" left="15.0" right="10.0" top="5.0" />
                </padding>
               <children>
               	<Label id="taskListLabel" styleClass="list-label" text="Tasks">
                     <padding>
                        <Insets bottom="5.0" />
                     </padding></Label>
                  <AnchorPane fx:id="taskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab closable="false" text="This Week">
                     <content>
                        <AnchorPane fx:id="overviewTabAnchorPane" prefHeight="171.0" prefWidth="315.0">
                           <children>
                              <VBox fx:id="overview" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <AnchorPane fx:id="overviewPanelPlaceholder" prefWidth="315.0" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </content>
                  </Tab>
                <Tab closable="false" text="Reserved">
                  <content>
                    <AnchorPane fx:id="rsvTabAnchorPane" prefHeight="171.0" prefWidth="315.0">
                           <children>
                              <VBox fx:id="rsvTaskList" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <AnchorPane fx:id="rsvTaskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab closable="false" text="Help">
                  <content>
                    <AnchorPane fx:id="helpTabAnchorPane" prefHeight="171.0" prefWidth="315.0">
                           <children>
                              <AnchorPane fx:id="helpPanelPlaceholder" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </items>
      </SplitPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="100" minHeight="83.0" prefHeight="100.0" prefWidth="692.0" styleClass="anchor-pane" VBox.vgrow="NEVER">
           <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
           </padding>
       </AnchorPane>
      <AnchorPane fx:id="commandBoxPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="0" left="0" right="0" top="0" />
         </padding>
      </AnchorPane>
      <AnchorPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
   </children>
   <stylesheets>
   	  <URL value="@TarsTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
</VBox>
